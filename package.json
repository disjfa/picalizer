{
  "name": "picalizer",
  "version": "1.0.0",
  "description": "A light foundation for your next frontend project based on webpack.",
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors --host 0.0.0.0",
    "deploy": "gh-pages -d dist",
    "favicon": "gulp favicon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wbkd/yet-another-webpack-es6-starterkit.git"
  },
  "keywords": [
    "webpack",
    "startkit",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "webkid.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wbkd/yet-another-webpack-es6-starterkit/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "homepage": "https://github.com/wbkd/yet-another-webpack-es6-starterkit#readme",
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-regular": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "airbnb-js-shims": "^1.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^4.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fancy-log": "^1.3.2",
    "favicons-webpack-plugin": "0.0.9",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-favicons": "^2.2.7",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "string-width": "^2.1.1",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "workbox-webpack-plugin": "^3.0.1"
  }
}
